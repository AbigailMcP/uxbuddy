%h3
  %strong #{@test.name}
  \ // Share
%hr

%p.flow-text
  Congratulations. Your test is ready to share.

.row
  .col.s6
    %h5#url-title Share Your Test URL
    #url
      =link_to uxbuddy_test_url, "#{uxbuddy_test_url}", class: "active"
  .col.s6
    %h5 Email Your Test
    .col.s9#email-div
      %input{type: "text", name: :emails, placeholder: "Add emails here"}
    .col.s3
      %input{type: "submit", value: "Send", class: "btn waves-light waves-effect" }
